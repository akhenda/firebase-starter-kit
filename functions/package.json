{
  "name": "functions",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": "16",
    "npm": "Please Use PNPM instead of NPM",
    "yarn": "Please Use PNPM instead of Yarn"
  },
  "volta": {
    "node": "16.19.0",
    "pnpm": "7.21.0"
  },
  "packageManager": "pnpm@7.21.0",
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint --ext .js,.ts .",
    "build": "rm -rf lib && tsc && tsc-alias",
    "build:watch": "concurrently \"tsc --watch\" \"tsc-alias -w\"",
    "serve": "pnpm run build && firebase emulators:start --only functions",
    "shell": "pnpm run build && firebase functions:shell",
    "start": "pnpm run shell",
    "serve:watch": "concurrently --kill-others \"pnpm run build:watch\" \"firebase emulators:start --only functions\"",
    "shell:watch": "concurrently --kill-others \"pnpm run build:watch\" \"firebase functions:shell\"",
    "emulators": "firebase emulators:start",
    "emulators:test-data": "firebase emulators:start --import=./test-data",
    "serve:all": "pnpm run build:watch | firebase emulators:start",
    "serve:admin": "pnpm run build && firebase emulators:exec --ui --only functions,firestore 'pnpm run build:watch'",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log"
  },
  "dependencies": {
    "@apollo/server": "^4.3.0",
    "body-parser": "^1.20.1",
    "camelcase-keys": "^7.0.2",
    "chance": "^1.1.9",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-youch": "^1.0.1",
    "firebase-admin": "10.3.0",
    "firebase-functions": "4.1.1",
    "fs-extra": "^11.1.0",
    "graphql": "^16.6.0",
    "helmet": "^6.0.1",
    "json2csv": "^5.0.7",
    "libphonenumber-js": "^1.10.15",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "nocache": "^3.0.4",
    "omit-empty": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "serve-favicon": "^2.5.0",
    "typesaurus": "7.2.0",
    "unique-names-generator": "^4.7.1",
    "youch": "^3.2.2"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "@types/lodash": "^4.14.191",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.11.18",
    "@types/omit-empty": "^1.0.1",
    "@types/serve-favicon": "^2.5.3",
    "@typescript-eslint/eslint-plugin": "5.47.1",
    "@typescript-eslint/parser": "5.47.1",
    "concurrently": "7.6.0",
    "eslint": "8.31.0",
    "eslint-config-heimdall": "1.4.0",
    "firebase-functions-test": "3.0.0",
    "prettier": "2.8.1",
    "tsc-alias": "1.8.2",
    "typescript": "4.9.4"
  }
}
