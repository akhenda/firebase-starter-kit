{
  "name": "functions",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": "16",
    "npm": "Please Use PNPM instead of NPM",
    "yarn": "Please Use PNPM instead of Yarn"
  },
  "volta": {
    "node": "16.20.2",
    "pnpm": "7.33.6"
  },
  "packageManager": "pnpm@7.33.6",
  "main": "lib/src/index.js",
  "scripts": {
    "lint": "eslint --ext .js,.ts .",
    "build": "rm -rf lib && tsc && tsc-alias",
    "build:watch": "concurrently \"tsc --watch\" \"tsc-alias -w\"",
    "serve": "pnpm run build && firebase emulators:start --only functions",
    "shell": "pnpm run build && firebase functions:shell",
    "start": "pnpm run shell",
    "serve:watch": "concurrently --kill-others \"pnpm run build:watch\" \"firebase emulators:start --only functions\"",
    "shell:watch": "concurrently --kill-others \"pnpm run build:watch\" \"firebase functions:shell\"",
    "emulators": "firebase emulators:start",
    "emulators:test-data": "firebase emulators:start --import=./__tests__/__data__",
    "serve:all": "pnpm run build:watch | firebase emulators:start",
    "serve:admin": "pnpm run build && firebase emulators:exec --ui --only functions,firestore 'pnpm run build:watch'",
    "test": "jest",
    "test:ci": "jest | egrep -v 'INFO|ms - '",
    "test:emulator": "firebase emulators:exec --only auth,firestore,storage --export-on-exit=./__tests__/__data__ 'pnpm run test --watch'",
    "test:emulator:ci": "firebase emulators:exec 'pnpm run test:ci' && pnpm run coverage:ci",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log"
  },
  "dependencies": {
    "@apollo/server": "^4.3.0",
    "body-parser": "^1.20.1",
    "camelcase-keys": "^7.0.2",
    "chance": "^1.1.9",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-youch": "^1.0.1",
    "firebase": "^9.15.0",
    "firebase-admin": "10.3.0",
    "firebase-functions": "4.5.0",
    "fs-extra": "^11.1.0",
    "graphql": "^16.6.0",
    "helmet": "^6.2.0",
    "json2csv": "^5.0.7",
    "libphonenumber-js": "^1.10.15",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "nocache": "^3.0.4",
    "omit-empty": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "serve-favicon": "^2.5.0",
    "typesaurus": "7.2.0",
    "unique-names-generator": "^4.7.1",
    "youch": "^3.2.2"
  },
  "devDependencies": {
    "@firebase/rules-unit-testing": "2.0.7",
    "@jest/globals": "^29.5.0",
    "@types/body-parser": "^1.19.2",
    "@types/chance": "^1.1.3",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^29.5.2",
    "@types/lodash": "^4.14.191",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.19.3",
    "@types/omit-empty": "^1.0.1",
    "@types/serve-favicon": "^2.5.3",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "concurrently": "8.2.2",
    "eslint": "8.55.0",
    "eslint-config-heimdall": "1.6.0",
    "eslint-plugin-jest-extended": "^2.0.0",
    "firebase-functions-test": "3.1.0",
    "jest": "^29.3.1",
    "jest-extended": "^3.2.0",
    "prettier": "2.8.8",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.3",
    "tsc-alias": "1.8.8",
    "typescript": "4.9.5"
  }
}
