{
  "name": "functions",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": "16",
    "npm": "Please Use PNPM instead of NPM",
    "yarn": "Please Use PNPM instead of Yarn"
  },
  "volta": {
    "node": "16.19.0",
    "pnpm": "7.21.0"
  },
  "packageManager": "pnpm@7.21.0",
  "main": "lib/src/index.js",
  "scripts": {
    "lint": "eslint --ext .js,.ts .",
    "build": "tsc && tsc-alias",
    "build:watch": "concurrently \"tsc --watch\" \"tsc-alias -w\"",
    "serve": "pnpm run build && firebase emulators:start --only functions",
    "shell": "pnpm run build && firebase functions:shell",
    "start": "pnpm run shell",
    "serve:watch": "concurrently --kill-others \"pnpm run build:watch\" \"firebase emulators:start --only functions\"",
    "shell:watch": "concurrently --kill-others \"pnpm run build:watch\" \"firebase functions:shell\"",
    "emulators": "firebase emulators:start",
    "emulators:test-data": "firebase emulators:start --import=./test-data",
    "serve:all": "pnpm run build:watch | firebase emulators:start",
    "serve:admin": "pnpm run build && firebase emulators:exec --ui --only functions,firestore 'pnpm run build:watch'",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log"
  },
  "dependencies": {
    "firebase-admin": "^10.0.2",
    "firebase-functions": "^3.18.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "concurrently": "^7.6.0",
    "eslint": "^8.9.0",
    "eslint-config-heimdall": "^1.4.0",
    "firebase-functions-test": "^0.2.0",
    "prettier": "^2.8.1",
    "tsc-alias": "^1.8.2",
    "typescript": "^4.5.4"
  }
}
